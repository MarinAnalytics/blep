# Environment variables for blep-click (copy to .env and adjust)

# API endpoint to resolve visitor country (must support CORS). Default used if unset.
VITE_FLAG_API=https://ipapi.co/json/?fields=country_code,country_name

# Initial blep counter value (for testing/demo only)
VITE_INITIAL_BLEP_COUNT=0

# Audio volume (0.0 - 1.0)
VITE_AUDIO_VOLUME=1.0
