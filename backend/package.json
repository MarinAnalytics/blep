{
  "name": "blep-backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
  "test": "vitest run",
  "test:watch": "vitest",
    "migrate": "node-pg-migrate -m migrations -d $PGDATABASE",
    "migrate:up": "node-pg-migrate up -m migrations -d $PGDATABASE",
    "migrate:down": "node-pg-migrate down -m migrations -d $PGDATABASE",
    "migrate:create": "node-pg-migrate create -m migrations"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "node-pg-migrate": "^7.6.2",
  "nodemon": "^3.1.4",
  "supertest": "^6.3.4",
  "vitest": "^1.6.0"
  }
}
